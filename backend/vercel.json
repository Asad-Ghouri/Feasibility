{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "env": {
        "DATABASE_URL": "@database_url",
        "JWT_SECRET": "@jwt_secret"
    },
    "buildCommand": "npm install && npx prisma generate",
    "installCommand": "npm install",
    "outputDirectory": ".",
    "framework": "express"
}
